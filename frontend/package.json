{
  "name": "where-is-maurus-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "license": "AGPL-3.0",
  "scripts": {
    "dev:share": "vite --config vite.share.config.ts --open",
    "dev:admin": "vite --config vite.admin.config.ts --open",
    "i18n": "lingui extract --clean",
    "build:all": "rm -rf dist/ && npm run build:share && npm run build:admin",
    "build:share": "vite build --config vite.share.config.ts",
    "build:admin": "vite build --config vite.admin.config.ts",
    "build": "npm run build:share && npm run build:admin",
    "preview:share": "vite preview --config vite.share.config.ts --port 4173",
    "preview:admin": "vite preview --config vite.admin.config.ts --port 4174",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "knip": "knip"
  },
  "dependencies": {
    "@lingui/core": "^5.4.1",
    "@lingui/macro": "^5.4.1",
    "@lingui/react": "^5.4.1",
    "leaflet": "^1.9.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-leaflet": "^5.0.0",
    "react-sparklines": "^1.7.0",
    "tailwindcss": "^4.1.12",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@lingui/cli": "^5.4.1",
    "@lingui/vite-plugin": "^5.4.1",
    "@lingui/format-po": "^5.4.1",
    "@tailwindcss/vite": "^4.1.12",
    "@types/leaflet": "^1.9.20",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/react-sparklines": "^1.7.5",
    "@vitejs/plugin-react": "^5.0.0",
    "babel-plugin-macros": "^3.1.0",
    "eslint": "^9.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^6.0.0",
    "knip": "^5.63.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.5.0",
    "vite": "^7.1.11",
    "vite-plugin-pwa": "^1.0.2"
  }
}
