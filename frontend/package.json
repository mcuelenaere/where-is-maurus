{
  "name": "where-is-maurus-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev:share": "vite --config vite.share.config.ts --open",
    "dev:admin": "vite --config vite.admin.config.ts --open",
    "build:all": "rm -rf dist/ && npm run build:share && npm run build:admin",
    "build:share": "vite build --config vite.share.config.ts",
    "build:admin": "vite build --config vite.admin.config.ts",
    "build": "npm run build:share && npm run build:admin",
    "preview:share": "vite preview --config vite.share.config.ts --port 4173",
    "preview:admin": "vite preview --config vite.admin.config.ts --port 4174",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "dayjs": "^1.11.13",
    "leaflet": "^1.9.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "react-sparklines": "^1.7.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/leaflet": "^1.9.20",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/react-sparklines": "^1.7.5",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.5.0",
    "vite": "^7.0.0"
  }
}
