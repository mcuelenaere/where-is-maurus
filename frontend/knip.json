      "project": ["src/**/*.{ts,tsx}", "*.{ts,tsx,js,jsx}"],
      "ignoreDependencies": ["@lingui/macro", "tailwindcss"],
      "ignore": ["src/shared/api/types.ts"]
    }
  },
  "ignore": ["dist/**"]
}
