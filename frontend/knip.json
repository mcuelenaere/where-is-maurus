{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "workspaces": {
    ".": {
      "entry": [
        "src/apps/admin/main.tsx",
        "src/apps/share/main.tsx",
        "vite.admin.config.ts",
        "vite.share.config.ts",
        "lingui.config.ts"
