# API base URL (no trailing slash). If omitted, app will use relative paths
VITE_API_BASE_URL=https://api.example.com

# Comma-separated car IDs
VITE_CAR_IDS=1,2

# Defaults
VITE_DEFAULT_TTL_HOURS=8
VITE_DEFAULT_ARRIVE_RADIUS_M=100

# Map tiles
VITE_MAP_TILE_URL=https://tile.openstreetmap.org/{z}/{x}/{y}.png
VITE_MAP_ATTRIBUTION=&copy; OpenStreetMap contributors

# Optional: if you host the public share on another origin, provide a base for example URLs
# VITE_SHARE_BASE_URL=https://share.example.com


